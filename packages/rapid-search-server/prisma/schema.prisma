generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Queue {
  id    String @id @default(uuid()) @unique
  url   String

  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}

model Link {
  id String @id @default(uuid()) @unique

  url         String
  title       String?
  description String?
  tags        String?
  header1     String?
  copy1       String?

  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
