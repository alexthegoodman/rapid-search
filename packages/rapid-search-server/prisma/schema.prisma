generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Queue {
  id    String @id @default(uuid()) @unique
  url   String

  analyzedDate DateTime?

  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}

model Link {
  id String @id @default(uuid()) @unique

  url         String @unique
  title       String?
  description String?
  tags        String?
  header1     String?
  copy1       String?

  normalTitle       String?
  normalDescription String?

  lastAnalyzedDate DateTime @default(now())

  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}

model SearchQuery {
  id String @id @default(uuid()) @unique

  noramlText  String
  volume      Int

  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
